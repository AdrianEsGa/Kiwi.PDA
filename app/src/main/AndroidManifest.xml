<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.xander.kiwipda">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ViewModel.ProductTypesActivity"></activity>
        <activity android:name=".ViewModel.NewCommandActivity" />
        <activity android:name=".ViewModel.CommandDetailsActivity" />
        <activity android:name=".ViewModel.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ViewModel.EmployeesActivity" />
        <activity android:name=".ViewModel.TablesActivity" />
        <activity android:name=".ViewModel.CommandsActivity" />
    </application>

</manifest>